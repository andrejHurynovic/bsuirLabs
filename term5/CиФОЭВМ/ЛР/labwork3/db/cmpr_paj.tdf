--synthesis_resources = lut 2 
SUBDESIGN cmpr_paj
( 
	agb	:	output;
	dataa[1..0]	:	input;
	datab[1..0]	:	input;
) 
VARIABLE
	agb_int	:	WIRE;
BEGIN 
	IF (dataa[] > datab[]) THEN
		agb_int = VCC;
	ELSE
		agb_int = GND;
	END IF;
	agb = agb_int;
END;
